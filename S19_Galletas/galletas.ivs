	ScriptCode="// Position variables&#xD;var heartX, heartY, moonX, moonY, starX, starY;&#xD;&#xD;// Distance variables respect to heart position&#xD;var moonDistance, starDistance;&#xD;&#xD;// Loop variable&#xD;var i;&#xD;&#xD;// farthest cookie shape string&#xD;var cookieResult;&#xD;&#xD;// getting cookies position&#xD;for( i=0; i&lt;4; i++ ){&#xD;	if( Vars.Models[i] == &quot;ModelH&quot; ){&#xD;		heartX = Vars.Positions[i].x;&#xD;		heartY = Vars.Positions[i].y;&#xD;	}&#xD;	else if( Vars.Models[i] == &quot;ModelM&quot; ){&#xD;		moonX = Vars.Positions[i].x;&#xD;		moonY = Vars.Positions[i].y;&#xD;	}&#xD;	else if( Vars.Models[i] == &quot;ModelS&quot; ){&#xD;		starX = Vars.Positions[i].x;&#xD;		starY = Vars.Positions[i].y;&#xD;	}&#xD;}&#xD;&#xD;// calculating cookies distances&#xD;moonDistance = Math.sqrt( Math.pow( heartX - moonX , 2 ) + Math.pow( heartY - moonY, 2 ) );&#xD;starDistance = Math.sqrt( Math.pow( heartX - starX , 2) + Math.pow( heartY - starY , 2) );&#xD;&#xD;// checking farthest cookie&#xD;if(moonDistance &gt;= starDistance){&#xD;	cookieResult = &quot;Luna&quot;;&#xD;}&#xD;else{&#xD;	cookieResult = &quot;Estrella&quot;;&#xD;}&#xD;&#xD;// displaying result&#xD;Vars.distancesStr = &quot;Luna = &quot; + moonDistance.toFixed(2) + &quot;, Estrella = &quot; + starDistance.toFixed(2);&#xD;Vars.resultStr = &quot;Galleta mas lejana : &quot; + cookieResult;">
